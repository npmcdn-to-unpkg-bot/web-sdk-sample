{
  "ecmaFeatures": {
    "jsx": true,
    "modules": true
  },
  "env": {
    "browser": true,
    "node": true
  },
  "parser": "babel-eslint",
  "rules": {
    "quotes": [2, "single"],
    "strict": [2, "never"],
    "react/jsx-uses-react": 2,
    "react/jsx-uses-vars": 2,
    "react/react-in-jsx-scope": 2,
    "arrow-body-style": 0,
    "react/jsx-no-bind": 0,
    "object-shorthand": 0,
    "no-var": 0,
    "vars-on-top": 0,
    "prefer-template": 0
  },
  "plugins": [
    "react"
  ],
  "globals": {
    "VoxeetSdk": false,
    "voxeet": false,
  },
  "extends": "defaults/configurations/airbnb/es6-react"
}
